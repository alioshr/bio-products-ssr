#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules/terser/bin/terser/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules/terser/bin/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules/terser/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/terser/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules:/Users/Dev/Desktop/node_modules:/Users/Dev/node_modules:/Users/node_modules:/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/node_modules/terser/bin/terser/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/node_modules/terser/bin/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/node_modules/terser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else 
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
