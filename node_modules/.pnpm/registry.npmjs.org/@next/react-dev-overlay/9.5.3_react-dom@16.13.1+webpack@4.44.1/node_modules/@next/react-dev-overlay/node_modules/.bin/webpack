#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/webpack/4.44.1_webpack@4.44.1/node_modules/webpack/bin/webpack.js/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/webpack/4.44.1_webpack@4.44.1/node_modules/webpack/bin/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/webpack/4.44.1_webpack@4.44.1/node_modules/webpack/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/webpack/4.44.1_webpack@4.44.1/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/webpack/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules:/Users/Dev/Desktop/node_modules:/Users/Dev/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../../webpack/4.44.1_webpack@4.44.1/node_modules/webpack/bin/webpack.js" "$@"
else 
  exec node  "$basedir/../../../../../../../../webpack/4.44.1_webpack@4.44.1/node_modules/webpack/bin/webpack.js" "$@"
fi
