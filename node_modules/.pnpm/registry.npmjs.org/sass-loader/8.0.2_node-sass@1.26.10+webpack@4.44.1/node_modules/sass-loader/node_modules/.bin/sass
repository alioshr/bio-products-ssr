#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/sass/1.26.10/node_modules/sass/sass.js/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/sass/1.26.10/node_modules/sass/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/sass/1.26.10/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/sass/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules:/Users/Dev/Desktop/node_modules:/Users/Dev/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../sass/1.26.10/node_modules/sass/sass.js" "$@"
else 
  exec node  "$basedir/../../../../../../sass/1.26.10/node_modules/sass/sass.js" "$@"
fi
