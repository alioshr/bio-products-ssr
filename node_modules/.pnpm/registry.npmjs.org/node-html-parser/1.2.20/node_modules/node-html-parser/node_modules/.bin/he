#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/he/1.1.1/node_modules/he/bin/he/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/he/1.1.1/node_modules/he/bin/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/he/1.1.1/node_modules/he/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/he/1.1.1/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/he/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules/.pnpm/node_modules:/Users/Dev/Desktop/bio-products-ssr/node_modules:/Users/Dev/Desktop/node_modules:/Users/Dev/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../he/1.1.1/node_modules/he/bin/he" "$@"
else 
  exec node  "$basedir/../../../../../../he/1.1.1/node_modules/he/bin/he" "$@"
fi
